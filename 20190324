#include <iostream>
#include <limits.h>

using namespace std;

int main()
{
    cout<<"Hello World";
    signed char aa = __SCHAR_MAX__;
    short int bb = __SHRT_MAX__;
    long cc = __LONG_MAX__;
    int dd = __INT_MAX__;
    cout<<aa<<" "<<bb<<" "<<cc<<" "<<dd;
    return 0;
}

